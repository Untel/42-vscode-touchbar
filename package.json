{
	"name": "ext",
	"displayName": "42_vscode_touchbar",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.47.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:42-vscode-touchbar.openGroup",
		"onCommand:42-vscode-touchbar.closeGroup",
		"onCommand:42-vscode-touchbar.norminette",
		"onCommand:42-vscode-touchbar.comment",
		"onCommand:42-vscode-touchbar.header"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
		{
			"command": "42-vscode-touchbar.header",
			"group": "extGroup",
			"title": "1 Header",
			"icon": "./assets/42_logo_white3.png"
		},
		{
			"command": "42-vscode-touchbar.comment",
			"group": "extGroup",
			"icon": "./assets/42_comment3.png",
			"title": "2 Comment"
		}
		],
		"menus": {
			"touchBar": [
				{
					"command": "42-vscode-touchbar.header",
					"group": "extGroup",
					"when": "editorTextFocus"
				},
				{
					"command": "42-vscode-touchbar.comment",
					"group": "extGroup",
					"when": "editorTextFocus"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.47.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.30.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}
